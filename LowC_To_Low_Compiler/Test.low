subroutine Wait_For_Vblank
{
	SP += 254;		# byte* Register;
	SP += 255;		# byte Scanline;
	BC = $FF44;

	label __do_while_statement__0;		# do/while loop label
	H = B;		# Creates copy of Register
	L = C;		# Creates copy of Register
	A = [HL];		# Dereferences address
	A < $90;		# S_LESS_THAN8
	B = H;		# Moves value into expected register
	C = L;		# Moves value into expected register
	jump carry __do_while_statement__0;		# do/while loop jump

	HL = $FF40;
	[HL] = 0; 		# Stores value into memory location
	SP += 3;		# Remove local variables from stack
	return;
}

subroutine Load_Graphics_Into_VRAM
{
	SP += 254;		# byte* Write_Head;
	SP += 254;		# byte* Read_Head;
	SP += 255;		# byte Counter;
	BC = $8000;
	DE = Brick_Graphics;
	A = 208;

	A |= A;		# This just gets the CPU flags
	jump zero __if_statement__1;		# If statement jump

	label __do_while_statement__2;		# do/while loop label
	H = D;		# Creates copy of Read_Head
	L = E;		# Creates copy of Read_Head
	E = [HL];		# Dereferences address
	push HL;		# Push 16-bit word to stack to free up registers
	H = B;		# Creates copy of Write_Head
	L = C;		# Creates copy of Write_Head
	[HL] = E; 		# Stores value into memory location
	pop BC;		# Fixes 'panic' push, places value back into register
	HL++;		# Increments Write_Head
	BC++;		# Increments Read_Head
	A--;		# Decrements Counter
	A |= A;		# This just gets the CPU flags
	E = B;		# Temporarily stores value here
	B = H;		# Moves value into expected register
	H = E;		# Swaps temporarily value
	E = C;		# Temporarily stores value here
	C = L;		# Moves value into expected register
	L = E;		# Swaps temporarily value
	D = H;		# Moves value into expected register
	E = L;		# Moves value into expected register
	jump not_zero __do_while_statement__2;		# do/while loop jump

	label __if_statement__1;		# If statement label

	SP += 5;		# Remove local variables from stack
	return;
}

subroutine main
{
	call Wait_For_Vblank;		# Calls function
	call Load_Graphics_Into_VRAM;		# Calls function
	return;
}

